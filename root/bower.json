{
  "name"		: "{%= name %}",
  "version"	: "0.0.0",
  "homepage": "https://github.com/FCOO/{%= name %}",
  "authors": [
    "{%= author_name %} <niels@steenbuchholt.dk>"
  ],
  "description": "{%= description %}",
  "main": [
    "dist/{%= name %}.min.js",
    "dist/{%= name %}.min.css"
  ],
	"repository": {
		"type": "git",
		"url": "git://github.com/FCOO/{%= name %}.git"
	},

	"dependencies": {
		"jquery": "latest",
		"moment": "latest"
	},

	"overrides": {
		"modernizr"											: { "main"	: "modernizr.js" },
		"BOWER_PACKAGE_NOT_TO_BE_USED"	: { "ignore": true },
		"webshim"												: { "main"	: "js-webshim/minified/polyfiller.js" }
	},

	"devDependencies": {
    "bourbon": "latest",
		"modernizr-mixin": "latest",
		"mathsass": "latest"
  },
	
	"license": "MIT",

  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "moduleType": [
    "es6"
  ]
}
